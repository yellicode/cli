    "@yellicode/templating": "^1.8.0",
    "@yellicode/typescript": "^1.13.3",
    "await-exec": "^0.1.2",
    "child_process": "^1.0.2",
    "fs": "0.0.1-security",
    "inquirer": "^8.1.1",
    "nodegit": "^0.27.0",
    "ora": "^5.3.0",
    "semantic-release": "^17.3.8",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.14",
